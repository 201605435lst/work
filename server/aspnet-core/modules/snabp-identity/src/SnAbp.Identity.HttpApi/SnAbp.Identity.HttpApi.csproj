  <ItemGroup>
    <ProjectReference Include="..\SnAbp.Identity.Application.Contracts\SnAbp.Identity.Application.Contracts.csproj" />
    <PackageReference Include="Volo.Abp.AspNetCore.Mvc" Version="3.0.5" />
